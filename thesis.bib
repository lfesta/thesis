@MISC{Comment:molasses,
    title = "{\rm After loading in the dark-spot MOT we keep atoms for 5 ms in an optical molasses to reduce temperature.
    Magnetic fields are switched off, cooling light detuning is linearly ramped from $-\Gamma$ to $-3\Gamma$ and its intensity decreased by 20\%.}",
}

@MISC{Comment:field,
    title = "{\rm We believe that the main source of error in the simplified model comes from the computed magnetic field. In fact the vacuum system
    weldings show some finite magnetization and being around the cooling area they might modify the actual field. In addition the model considers only the
    field along the central vertical axis, but atoms feel the slowing force on a circle of 8 mm radius, where the maximum field along a vertical line can vary by
    more than 10\%.}",
}

